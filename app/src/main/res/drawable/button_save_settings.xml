<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="50dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#4FFF0F"
        android:startColor="#B5E80E"
        android:endColor="#07751D"
        android:type="linear"
        />
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        />
    <stroke
        android:width="2dp"
        android:color="#0D330A"
        />
</shape>